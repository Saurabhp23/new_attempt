<h1><strong>THIS IS USER LISTING PAGE</strong></h1>
<table>
  <thead>
    <th>Name</th>
    <th>About</th>
    <th>Action</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.about %></td>
        <td><%= link_to 'Show', user_path(user) %></td>
        <td><%= link_to 'Delete', user_path(user), method: :delete %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<p>
  <%= link_to 'Create new user', new_user_path %>
</p>